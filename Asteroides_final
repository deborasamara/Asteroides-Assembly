	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8)
	
	# pixel 16 - diagonal direita baixo
	addi $8, $8, -508
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8)
	
	# pixel 17 - diagonal direita baixo
	addi $8, $8, -508

	sw $9, 32768($8)
	
	# pixel 21 - diagonal direita cima
	addi $8, $8, -516
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8)
	
	# pixel 22 - diagonal direita cima
	addi $8, $8, -516
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8)
	
	# fazer o asteroide voltar pra posição inicial quando chegar no fim
	
	slt $10, $11, $14
	bne $10, $0, asteroideV2PosInicial
	
continuaDeleteAsteroideV2:
	# finalizando
	add $31, $0, $12 # recuperando o valor de $31
	jr $31
	
criarNave: # Criar nave
	add $12, $0, $31 # salvar valor do $31 - Cópia de segurança
	jal resetOito # reseta o reg8
	add $8, $8, $15
	
	# ponto 1 - base triangulo
	addi $8, $8, 17648 # Contas para transformar o valor do $13 no novo valor
	addi $9, $0, 0xffffff # cor
	sw $9, 0($8)
	
	#	ponto 2 - base triangulo
	addi $8, $8, 4#17652
	sw $9, 0($8)
	#	ponto 3 - base triangulo
	addi $8, $8, 4 #17656
	sw $9, 0($8)
	#	ponto 4 - base triangulo
	addi $8, $8, 4#17660
	sw $9, 0($8)
	#	ponto 5 - base triangulo
	addi $8, $8, 4#17664
	sw $9, 0($8)
	#	ponto 6 - base triangulo
	addi $8, $8, 4#17668
	sw $9, 0($8)
	#	ponto 7 - base triangulo
	addi $8, $8, 4#17672
	sw $9, 0($8)
	#	ponto 8 - lado esquerdo triangulo 
	addi $8, $8, -532 #17140
	sw $9, 0($8)
	#	ponto 9 - lado esquerdo triangulo 
	addi $8, $8, -508 #16632
	sw $9, 0($8)
	#	ponto 10 - lado direito triangulo 
	addi $8, $8, 8 #16640
	sw $9, 0($8)
	#	ponto 11 - lado direito triangulo 
	addi $8, $8, 516 #17156
	sw $9, 0($8)
	#	ponto 12 - ponta de cima do triangulo (o de baixo)
	addi $8, $8, -1032#16124
	sw $9, 0($8)
	#	ponto 13 - ponta de cima do triangulo (o de cima)
	addi $8, $8, -512#15612
	sw $9, 0($8)
	
	# finalizando
	add $31, $0, $12  # devolve valor para o reg31
	jr $31
	
deletarNave:
	add $12, $0, $31 # salvar valor do $31 - Cópia de segurança
	jal resetOito
	add $8, $8, $15
	
	# pixel 1 - parte de cima
	addi $8, $8, 17648 # Contas para transformar o valor do $13 no novo valor
	lw $9, 32768($8) # tirando a cor da shadown
	sw $9, 0($8)	# coloca no cenário principal
	sw $9, 32768($8) # devolve a cor pro shadown
	#	ponto 2 - base triangulo
	addi $8, $8, 4
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	#	ponto 3 - base triangulo
	addi $8, $8, 4 #17656
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	#	ponto 4 - base triangulo
	addi $8, $8, 4#17660
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	#	ponto 5 - base triangulo
	addi $8, $8, 4#17664
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	#	ponto 6 - base triangulo
	addi $8, $8, 4#17668
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	#	ponto 7 - base triangulo
	addi $8, $8, 4#17672
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	#	ponto 8 - lado esquerdo triangulo 
	addi $8, $8, -532 #17140
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	#	ponto 9 - lado esquerdo triangulo 
	addi $8, $8, -508 #16632
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	#	ponto 10 - lado direito triangulo 
	addi $8, $8, 8 #16640
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	#	ponto 11 - lado direito triangulo 
	addi $8, $8, 516 #17156
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	#	ponto 12 - ponta de cima do triangulo (o de baixo)
	addi $8, $8, -1032#16124
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	#	ponto 13 - ponta de cima do triangulo (o de cima)
	addi $8, $8, -512#15612
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8) 
	
	# finalizando
	add $31, $0, $12  # devolve valor para o reg31
	jr $31

resetOito:
	lui $8, 0x1001
	jr $31

# Dar um tempo
time:	
	addi $20, $0, 20000
foriniciotime:
	beq $20, $0, fimTime
        nop
        addi $20, $20, -1 
        j foriniciotime
fimTime:
	jr $31



#Fim do jogo
	
fim:
fimmesmo:
	#add $12, $0, $31 # salvar valor do $31 - Cópia de segurança
	#jal resetOito # reseta o reg8
	#add $8, $8, $15
	# tela de fim de jogo
	# FIM VITÓRIA
	lui $8, 0x1001 # move 16 bits e adiciona primeira posição

# pinta tela de 8 cores - efeito background
# usa sem preservar: $9 - $10
# $8 - relacionado a posição na memória
# $9 - fica com o código das cores
# $10 - funciona como um registrador de iteração 

# CENÁRIO DO JOGO
	addi $10, $0, 1024 # quantidade de pixels de cada faixa de cor. 128 x 8 = 1024
	addi $9, $0, 0x000000 # faixa de cor 1
forf1:	beq $10, $0, forf2i
	sw $9, 0($8)
	sw $9, 32768($8)# SHADOWN
	addi $8, $8, 4
	addi $10, $10, -1
	j forf1
	
forf2i:	addi $10, $0, 1024
	addi $9, $0, 0x040211 # faixa de cor 2	
forf2:	beq $10, $0, forf3i
	sw $9, 0($8)
	sw $9, 32768($8)# SHADOWN
	addi $8, $8, 4
	addi $10, $10, -1
	j forf2
	
forf3i:	addi $10, $0, 1024
	addi $9, $0, 0x090422 # faixa de cor 3	
forf3:	beq $10, $0, forf4i
	sw $9, 0($8)
	sw $9, 32768($8)# SHADOWN
	addi $8, $8, 4
	addi $10, $10, -1
	j forf3
	
forf4i:  addi $10, $0, 1024
	addi $9, $0, 0x0d0634 # faixa de cor 4
forf4:	beq $10, $0, forf5i
	sw $9, 0($8)
	sw $9, 32768($8)# SHADOWN
	addi $8, $8, 4
	addi $10, $10, -1
	j forf4
	
forf5i:	addi $10, $0, 1024
	addi $9, $0, 0x120845 # faixa de cor 5
forf5:	beq $10, $0, forf6i
	sw $9, 0($8)
	sw $9, 32768($8)# SHADOWN
	addi $8, $8, 4
	addi $10, $10, -1
	j forf5
	
forf6i:  addi $10, $0, 1024
	addi $9, $0, 0x160a57 # faixa de cor 6
forf6:	beq $10, $0, forf7i
	sw $9, 0($8)
	sw $9, 32768($8)# SHADOWN
	addi $8, $8, 4
	addi $10, $10, -1
	j forf6
	
forf7i:  addi $10, $0, 1024
	addi $9, $0, 0x1b0c68 # faixa de cor 7
forf7:	beq $10, $0, forf8i
	sw $9, 0($8)
	sw $9, 32768($8)# SHADOWN
	addi $8, $8, 4
	addi $10, $10, -1
	j forf7
	
forf8i:  addi $10, $0, 1024
	addi $9, $0, 0x1f0e7a # faixa de cor 8
forf8:	beq $10, $0, continuaestrelasf
	sw $9, 0($8)
	sw $9, 32768($8)# SHADOWN
	addi $8, $8, 4
	addi $10, $10, -1
	j forf8
continuaestrelasf:
	# pinta estrelas

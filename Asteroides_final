	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8)
	
	# pixel 13 - parte de baixo 
	addi $8, $8, 4
	lw $9, 32768($8) 
	sw $9, 0($8)	
	sw $9, 32768($8)
	
	# pixel 14 - parte de baixo

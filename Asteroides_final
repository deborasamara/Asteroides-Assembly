	jal time
	lui $1, 0xffff  # reg para ver se o user digitou algo
	lw $17, 4($1) # se for diferente de zero, algo foi digitado.
	addi $18, $0, 'r' 
	beq $17, $0, fimdeverdade # checar se o user digitou alguma coisa
	beq $17, $18, reiniciodojogo

fimdeverdade:
	# acaba jogo
	addi $2, $0, 10
	syscall
	
# Outras funcionalidades
	
asteroidePosInicial:
	# colocar na posição inicial
	addi $13, $0, -4868
	add $7, $0, $0 # reinicia 
	
	j continuaDeleteAsteroideTres # asteroide Tres
	
	
asteroideV2PosInicial:
	addi $14, $0, -4992
	
	j continuaDeleteAsteroideV2
	


	
	
	
	
	
	
	
	

